<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.what2do">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.What2do">

        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activities.ProfileActivity" />
        <activity android:name=".activities.GroupActivity" />
        <activity android:name=".activities.GroupInfoActivity" />
        <activity android:name=".activities.FactorsActivity" />
        <activity android:name=".activities.SwipeGenreActivity" />
        <activity android:name=".activities.SwipeActivity" />
        <activity android:name=".activities.SwipeInfoActivity" />
        <activity android:name=".activities.MatchesActivity" />
        <activity android:name=".activities.SingleMatchActivity" />
    </application>

</manifest>